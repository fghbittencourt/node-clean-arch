    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-perfectionist": "^2.11.0",
    "fishery": "^2.2.2",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^3.1.4",
