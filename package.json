  "packageManager": "pnpm@9.3.0",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "swc ./src -d ./dist",
    "start": "node ./src/index.js",
    "start:dist": "node ./dist/src/index.js",
    "dev:webapi": "nodemon --exec swc-node ./src/index.ts webapi",
    "migration:create": "typeorm-ts-node-esm migration:create src/repositories/postgres/migrations/change-my-name",
    "migration:generate": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:generate --dataSource ./src/repositories/postgres/appDataSource.ts src/repositories/postgres/migrations/change-my-name",
    "migration:run": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:run --dataSource ./src/repositories/postgres/appDataSource.ts",
    "migration:revert": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert --dataSource ./src/repositories/postgres/appDataSource.ts",
    "test": "jest --silent --maxWorkers=2"
  },
  "dependencies": {
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^4.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "semantic-release": "^24.0.0",
    "snakecase-keys": "^8.0.1",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "@faker-js/faker": "^8.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.6.1",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
